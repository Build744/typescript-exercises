{
  "name": "typescript-exercises",
  "version": "1.0.0",
  "description": "TypeScript exercises with Vite and Vitest testing framework",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "type-check": "tsc --noEmit --strict",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "exo:check": "node scripts/check-exercise.js",
    "exo:test": "node scripts/test-exercise.js",
    "exo:verify": "node scripts/run-exercise.js"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "prettier": "^3.6.2",
    "type-assertions": "^1.1.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "keywords": [
    "typescript",
    "exercises",
    "learning",
    "vite",
    "vitest"
  ],
  "author": "Build744",
  "license": "MIT"
}
